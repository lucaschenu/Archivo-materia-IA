        "#importat a base de dados\n",
        "from google.colab import files\n",
        "uploaded = files.upload()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 73
        },
        "id": "zuzhUHWLhRZR",
        "outputId": "27d16bbf-f13e-492a-f42c-8d0bb46fbe34"
