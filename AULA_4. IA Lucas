        "#separar as coluna pSpintor::VAX_speed (y, target) das demais coluns(x, preditores)\n",
        "X= df.drop([\"pSpintor::VAX_speed\", \"pCut::Motor_Torque\"], axis=1)\n",
        "Y= df[\"pSpintor::VAX_speed\"]"
      ],
      "metadata": {
        "id": "FZ5pcK3kmKDm"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# transformar a variavel categorica y em numerico (classificação binária)\n",
        "# Assuming the goal is binary classification based on the two most frequent values\n",
        "most_frequent_values = Y.value_counts().nlargest(2).index.tolist()\n",
        "y_binario = Y.apply(lambda x: 1 if x == most_frequent_values[0] else (0 if x == most_frequent_values[1] else -1))\n",
        "\n",
        "# Remove rows where the target is -1 (values other than the two most frequent)\n",
        "X = X[y_binario != -1]\n",
        "y_binario = y_binario[y_binario != -1]"
      ],
      "metadata": {
        "id": "AMwYotvqmp1l"
      },
